{
    "scripts": {
        "count": "cloc . --exclude-dir=node_modules,dist,next --exclude-ext=json",
        "scrape:all": "cd ./scrapers && npm run scrape:prod -- --all",
        "scrape:canada": "cd ./scrapers && npm run scrape:prod -- --canada",
        "scrape:alberta": "cd ./scrapers && npm run scrape:prod -- --canada --province=alberta",
        "scrape:british_columbia": "cd ./scrapers && npm run scrape:prod -- --canada --province=british_columbia",
        "scrape:ontario": "cd ./scrapers && npm run scrape:prod -- --canada --province=ontario",
        "scrape:quebec": "cd ./scrapers && npm run scrape:prod -- --canada --province=quebec",
        "scrape:usa": "cd ./scrapers && npm run scrape:prod -- --usa",
        "scrape:california": "cd ./scrapers && npm run scrape:prod -- --usa --state=california",
        "scrape:new_york": "cd ./scrapers && npm run scrape:prod -- --usa --state=new_york"
    }
}
